<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="kr.co.openkitchen.dto.MemberDTO" >
	<insert id="signUpInsert" >
		INSERT INTO member (
			M_EMAIL,
			M_PWD,
			M_NAME
			)
		VALUES (
			#{m_Email},
			#{m_Pwd},
			#{m_Name}
			)
	</insert>
</mapper>